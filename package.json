{
  "name": "mais-inventory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate --schema=./src/db/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./src/db/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/db/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/db/schema.prisma",
    "prisma:seed": "tsx src/db/seed.ts",
    "db:setup": "prisma migrate dev --schema=./src/db/schema.prisma && npm run prisma:seed"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@prisma/client": "^6.1.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "next": "15.1.3",
    "next-auth": "^5.0.0-beta.25",
    "next-intl": "^3.26.2",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "prisma": "^6.1.0",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
